  $endif$
  </div>
</div>

<div id="title-block">
  <h1 class="title">$if(h1)$$h1$$else$$title$$endif$</h1>
  $if(subtitle)$
  <div class="subtitle">$subtitle$</div>
  $endif$
  $for(author)$
  <div class="author">$author$</div>
  $endfor$
  $if(date)$
  <p class="date">$date$</p>
  $endif$
</div>

</header>